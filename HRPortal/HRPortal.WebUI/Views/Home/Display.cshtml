@model IEnumerable<HRPortal.Models.Application>

@{
    ViewBag.Title = "Display";
}

<div class="container">
    <h2>All Applications</h2>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var app in Model)
            {
                @Html.Partial("_Display", app)
            }
</tbody>
</table>
</div>

