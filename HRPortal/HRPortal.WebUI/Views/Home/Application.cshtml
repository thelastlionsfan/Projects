@model HRPortal.Models.Application


<form class="form-horizontal">
    <div class="form-group">
        <label class="col-sm-2 control-label">First Name</label>
        <div class="col-sm-10">
            <p class=" = form-control-static">
                @Html.DisplayFor(x => x.FirstName)
            </p>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 control-label">Last Name</label>
        <div class="col-sm-10">
            <p class=" = form-control-static">
                @Html.DisplayFor(x => x.LastName)
            </p>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 control-label">Phone Number</label>
        <div class="col-sm-10">
            <p class=" = form-control-static">
                @Html.DisplayFor(x => x.PhoneNumber)
            </p>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 control-label">Address</label>
        <div class="col-sm-10">
            <p class=" = form-control-static">
                @Html.DisplayFor(x => x.Address)
            </p>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 control-label">City</label>
        <div class="col-sm-10">
            <p class=" = form-control-static">
                @Html.DisplayFor(x => x.City)
            </p>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 control-label">Zip Code</label>
        <div class="col-sm-10">
            <p class=" = form-control-static">
                @Html.DisplayFor(x => x.Zip)
            </p>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 control-label">State</label>
        <div class="col-sm-10">
            <p class=" = form-control-static">
                @Html.DisplayFor(x => x.State)
            </p>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 control-label">Job History</label>
        <div class="col-sm-10">
            <p class=" = form-control-static">
                @Html.DisplayFor(x => x.JobHistory)
            </p>
        </div>
    </div>
</form>